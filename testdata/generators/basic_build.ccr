build(
  name      = "simple_build",
  steps     = [
    step.unpack_gz(to = 'src', path = 'cool.tar.gz'),
  ],
  output    = {
    'src/cool.txt': 'cool.txt',
  },
)


resource(
  name   = "output",
  parent = "common://resources:file",
  path   = "cool.txt",
  source = ":simple_build",
)

resource(
  name   = "output_missing_in_build",
  parent = "common://resources:file",
  path   = "missing.txt",
  source = ":simple_build",
)
